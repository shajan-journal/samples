{
  "name": "agent-patterns-workspace",
  "version": "0.1.0",
  "description": "Agentic AI Patterns - Workspace root",
  "private": true,
  "workspaces": [
    "api",
    "ui"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev:api": "npm run start:api --workspace api",
